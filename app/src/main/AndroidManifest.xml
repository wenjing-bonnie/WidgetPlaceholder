<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.android.widgetplaceholder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo.Light">
        <activity android:name="com.android.placeholder.PlaceHolderListViewActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- 生命周期 -->
        <activity
            android:name="com.android.viewlife.ViewLifeCycleActivity"
            android:label="生命周期" />
        <activity android:name="com.android.viewlife.OtherActivity" /> <!-- 验证属性的优先级 -->
        <activity
            android:name="com.android.attrsetting.GridViewAttrSettingActivity"
            android:label="属性设置" />
        <activity
            android:name="com.android.attrsetting.GridViewAttrTheme1SettingActivity"
            android:theme="@style/GridViewTheme" />
        <activity
            android:name="com.android.attrsetting.GridViewAttrTheme2SettingActivity"
            android:theme="@style/GridViewThemeAttr0" />
        <activity
            android:name="com.android.attrsetting.GridViewAttrTheme3SettingActivity"
            android:theme="@style/GridViewThemeAttr1" />
        <activity
            android:name="com.android.placeholder.PlaceHolderActivity"
            android:label="预占位" />
    </application>

</manifest>